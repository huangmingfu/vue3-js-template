{
  "name": "vue3-js-template",
  "private": true,
  "version": "2.0.0",
  "description": "一套基于vue3、js、vite5的项目模板，封装了axios，vue-router，scss，配置了vite.config.js。",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "build:pro": "vite build --mode pro",
    "build:test": "vite build --mode test",
    "build:dev": "vite build --mode dev",
    "preview": "vite preview",
    "deploy": "pnpm run build:pro && git add . && git commit -m \"gitee自动部署\" && git push -u origin master && git subtree push --prefix dist-pro origin gh-pages"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "3.0.3",
    "sass": "^1.69.0",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^5.2.7",
    "vite-plugin-html": "^3.2.0"
  }
}
